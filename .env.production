# Production Environment
APP_VARIANT=production
APP_VERSION=1.0.0
IOS_BUILD_NUMBER=1
ANDROID_VERSION_CODE=1

# API Configuration
API_URL=https://api.parking-app.com
API_KEY=production_api_key_secure_abcdef123456

# Firebase Configuration (Production)
FIREBASE_API_KEY=AIzaSyProductionKey123456789
FIREBASE_AUTH_DOMAIN=parking-app-hn.firebaseapp.com
FIREBASE_PROJECT_ID=parking-app-hn
FIREBASE_STORAGE_BUCKET=parking-app-hn.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789014
FIREBASE_APP_ID=1:123456789014:ios:prod123456789
FIREBASE_MEASUREMENT_ID=G-PRODUCTION1234

# Stripe Configuration (Live Keys)
STRIPE_PUBLISHABLE_KEY=pk_live_production_key_here

# EAS Configuration
EAS_PROJECT_ID=parking-app-expo

# App Store Connect (Production)
APPLE_ID=app@parking-app.com
ASC_APP_ID=1234567892
APPLE_TEAM_ID=ABC1234567

# Google Play Console (Production)
GOOGLE_SERVICE_ACCOUNT_KEY=./keys/google-service-account-prod.json
GOOGLE_SERVICES_JSON=./firebase-configs/google-services.json
GOOGLE_SERVICE_INFO_PLIST=./firebase-configs/GoogleService-Info.plist

# Feature Flags
ENABLE_DEBUG_MENU=false
ENABLE_TESTING_TOOLS=false
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_REMOTE_CONFIG=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEEP_LINKING=true

# Logging Configuration
LOG_LEVEL=error
ENABLE_CONSOLE_LOGS=false
ENABLE_FILE_LOGS=true
ENABLE_REMOTE_LOGGING=true

# Security Configuration
ENABLE_SSL_PINNING=true
API_TIMEOUT=15000
MAX_RETRY_ATTEMPTS=2

# Cache Configuration
CACHE_TIMEOUT=1800000
ENABLE_OFFLINE_MODE=true
MAX_CACHE_SIZE=200

# Development Tools
ENABLE_FLIPPER=false
ENABLE_REACTOTRON=false
ENABLE_DEV_SERVER=false

# Maps & Location
MAPS_API_KEY=AIzaSyProductionMapsKey123456789
DEFAULT_LOCATION_LAT=14.0723
DEFAULT_LOCATION_LNG=-87.1921
LOCATION_UPDATE_INTERVAL=15000

# Payment Configuration
STRIPE_MERCHANT_ID=merchant.com.parking.app
PAYMENT_TIMEOUT=120000

# Push Notifications
PUSH_NOTIFICATION_SENDER_ID=parking-app-hn
ONESIGNAL_APP_ID=onesignal-prod-app-id

# Deep Linking
DEEP_LINK_SCHEME=parking
UNIVERSAL_LINK_DOMAIN=parking-app.com

# Analytics
ANALYTICS_TRACKING_ID=G-PRODUCTION123456789
MIXPANEL_TOKEN=production_mixpanel_token_here

# Social Authentication
GOOGLE_WEB_CLIENT_ID=google-web-client-id-prod
FACEBOOK_APP_ID=facebook-app-id-prod
APPLE_SERVICE_ID=com.parking.app.signin