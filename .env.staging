# Staging Environment
APP_VARIANT=staging
APP_VERSION=1.0.0-staging
IOS_BUILD_NUMBER=1
ANDROID_VERSION_CODE=1

# API Configuration
API_URL=https://staging-api.parking-app.com
API_KEY=staging_api_key_secure_12345

# Firebase Configuration (Staging)
FIREBASE_API_KEY=AIzaSyStagingKey123456789
FIREBASE_AUTH_DOMAIN=parking-app-staging.firebaseapp.com
FIREBASE_PROJECT_ID=parking-app-staging
FIREBASE_STORAGE_BUCKET=parking-app-staging.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789013
FIREBASE_APP_ID=1:123456789013:ios:staging123456789
FIREBASE_MEASUREMENT_ID=G-STAGING1234

# Stripe Configuration (Test Keys)
STRIPE_PUBLISHABLE_KEY=pk_test_staging_key_here

# EAS Configuration
EAS_PROJECT_ID=parking-app-staging-expo

# App Store Connect (Staging)
APPLE_ID=developer@parking-app.com
ASC_APP_ID=1234567891
APPLE_TEAM_ID=ABC1234567

# Google Play Console (Staging)
GOOGLE_SERVICE_ACCOUNT_KEY=./keys/google-service-account-staging.json
GOOGLE_SERVICES_JSON=./firebase-configs/google-services-staging.json
GOOGLE_SERVICE_INFO_PLIST=./firebase-configs/GoogleService-Info-staging.plist

# Feature Flags
ENABLE_DEBUG_MENU=true
ENABLE_TESTING_TOOLS=true
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_REMOTE_CONFIG=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEEP_LINKING=true

# Logging Configuration
LOG_LEVEL=info
ENABLE_CONSOLE_LOGS=true
ENABLE_FILE_LOGS=true
ENABLE_REMOTE_LOGGING=true

# Security Configuration
ENABLE_SSL_PINNING=true
API_TIMEOUT=20000
MAX_RETRY_ATTEMPTS=3

# Cache Configuration
CACHE_TIMEOUT=600000
ENABLE_OFFLINE_MODE=true
MAX_CACHE_SIZE=100

# Development Tools
ENABLE_FLIPPER=false
ENABLE_REACTOTRON=false
ENABLE_DEV_SERVER=false

# Maps & Location
MAPS_API_KEY=AIzaSyStagingMapsKey123456789
DEFAULT_LOCATION_LAT=14.0723
DEFAULT_LOCATION_LNG=-87.1921
LOCATION_UPDATE_INTERVAL=10000

# Payment Configuration
STRIPE_MERCHANT_ID=merchant.com.parking.app.staging
PAYMENT_TIMEOUT=120000

# Push Notifications
PUSH_NOTIFICATION_SENDER_ID=parking-app-staging
ONESIGNAL_APP_ID=onesignal-staging-app-id

# Deep Linking
DEEP_LINK_SCHEME=parking-staging
UNIVERSAL_LINK_DOMAIN=staging.parking-app.com

# Analytics
ANALYTICS_TRACKING_ID=G-STAGING123456789
MIXPANEL_TOKEN=staging_mixpanel_token_here

# Social Authentication
GOOGLE_WEB_CLIENT_ID=google-web-client-id-staging
FACEBOOK_APP_ID=facebook-app-id-staging
APPLE_SERVICE_ID=com.parking.app.staging.signin